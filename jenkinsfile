pipeline{
    agent any
    stages {
        stage ("stage cleanup"){
            steps {
                sh "echo \"this is stage cleanup !\" "
            }
        }
        
        stage ("stage build"){
            steps {
                sh "echo \"this is stage build !\" "
            }
        }

        stage ("stage test"){
            steps {
                sh "echo \"this is stage test !\" "
            }
        }

        stage ("stage complete"){
            steps {
                sh "echo \"this is stage complete !\" "
            }
        }        
    }
}
